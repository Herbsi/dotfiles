"$schema" = "https://jj-vcs.github.io/jj/latest/config-schema.json"

[user]
name = {{ .git.name | quote }}
email = {{ .git.email | quote }}

[signing]
behavior = "drop"
backend = "ssh"
key = {{ .git.signingkey | quote }}

[git]
sign-on-push = true

[working-copy]
eol-conversion = "input-output"

[ui]
default-command = "log"
graph.style = "ascii"
